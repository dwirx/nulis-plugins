{
	"name": "nulis-plugin",
	"version": "1.0.0",
	"description": "Plugin Obsidian untuk membuat catatan dengan template otomatis dan pengorganisasian folder yang mudah",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"notes",
		"templates",
		"productivity",
		"markdown"
	],
	"author": "Hendra",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/nulis.git"
	},
	"bugs": {
		"url": "https://github.com/yourusername/nulis/issues"
	},
	"homepage": "https://github.com/yourusername/nulis#readme"
}
